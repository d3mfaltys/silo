[silo]
  loglevel        = "debug"
  port            = 8080
  content         = "http://test.com"
  domain          = "http://test.com"
  basedir         = "rkt"
  bootstrapdelay  = 1

[livereload]
  uselivereload   = true
  polldelay       = 2

[ssl]
  usetls          = false
  servercert      = deps/test.crt
  serverkey       = deps/test.key

[redis]
  host            = "localhost:6379"
  password        = ""
  cleanonboot     = true
